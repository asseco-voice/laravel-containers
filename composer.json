{
    "name": "asseco-voice/laravel-containers",
    "description": "Adding container support to Laravel models",
    "keywords": [
        "php",
        "laravel",
        "containers",
        "microservices",
        "database"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3.1"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Voice\\Containers\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Voice\\Containers\\ContainerServiceProvider"
            ]
        }
    }
}
