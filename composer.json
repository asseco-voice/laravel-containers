{
    "name": "asseco-voice/laravel-containers",
    "description": "Adding container support to Laravel models",
    "keywords": [
        "php",
        "laravel",
        "containers",
        "microservices",
        "database"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.4",
        "laravel/framework": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0",
        "mockery/mockery": "^1.3.1",
        "fzaninotto/faker": "^1.9.1",
        "orchestra/testbench":  "^6.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Asseco\\Containers\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Asseco\\Containers\\ContainerServiceProvider"
            ]
        }
    }
}
